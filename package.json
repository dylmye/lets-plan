  },
  "scripts": {
    "start": "craco start",
    "build": "PUBLIC_URL=https://lets-plan.ninja craco build",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "alphabetize": {
            "order": "desc",
            "caseInsensitive": true
          },
          "pathGroups": [
            {
              "pattern": "react",
              "group": "builtin",
              "position": "before"
            }
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.4.5",
    "react-device-detect/ua-parser-js": "1.0.33"
  },
  "prettier": {
    "singleQuote": false,
    "bracketSameLine": true
  },
  "lint-staged": {
    "*.{js,css,ts,tsx,json,html}": "prettier --write"
  }
}
